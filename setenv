#!/bin/bash

# LLVM 11.0.0
export LLVM_DIR=/root/decker/llvm-project
export LLVM_BIN=$LLVM_DIR/build/bin

export PROJ_DIR=/root/decker/whole-program-debloat
export PROJ_DEBRT_LIB=$PROJ_DIR/build/debloat_rt
export PROJ_PASS_WPD=$PROJ_DIR/build/passes/wpd

export PATH=$PATH:$LLVM_BIN
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$PROJ_DEBRT_LIB:$PROJ_PASS_WPD
export LIBRARY_PATH=$LIBRARY_PATH:$PROJ_DEBRT_LIB
